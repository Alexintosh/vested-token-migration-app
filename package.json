{
  "name": "@pteria-dao/vested-token-migration-app",
  "version": "0.1.0",
  "main": "index.js",
  "repository": "https://github.com/pie-dao/vested-token-migration-app.git",
  "author": "Mick de Graaf <mick@mickdegraaf.nl>",
  "license": "MIT",
  "scripts": {
    "test": "npx buidler test",
    "typechain": "typechain --target ethers-v5 --outDir typechain ./artifacts/**/*.json",
    "coverage": "yarn build && npx buidler coverage --network coverage --temp artifacts",
    "compile": "npx buidler compile --force --config buidler.compile.config.ts",
    "build": "yarn compile && yarn typechain",
    "getVestings": "node -r esm ./scripts/getVestings.js",
    "postinstall": "yarn build-app",
    "build-app": "cd app && npm install --force && cd ..",
    "start": "npx buidler start --network localhost",
    "clean": "rm -rf ./artifacts && rm -rf ./cache && rm -rf coverage",
    "publish:major:rinkeby": "npx buidler publish major --network rinkeby",
    "publish:minor:rinkeby": "npx buidler publish minor --network rinkeby",
    "publish:major:mainnet": "npx buidler publish major --network rinkeby",
    "publish:minor:mainnet": "npx buidler publish minot --network rinkeby"
  },
  "dependencies": {
    "@aragon/apps-shared-minime": "^1.0.2",
    "@aragon/apps-token-manager": "^2.1.0",
    "@aragon/buidler-aragon": "^0.2.9",
    "@aragon/os": "^4.4.0",
    "@nomiclabs/buidler-ethers": "^2.0.0",
    "@nomiclabs/buidler-etherscan": "^1.3.0",
    "@nomiclabs/buidler-truffle5": "^1.3.4",
    "@nomiclabs/buidler-web3": "^1.3.4",
    "@typechain/ethers-v5": "^1.0.0",
    "@types/mocha": "^8.0.2",
    "@types/node": "^14.0.27",
    "dotenv": "^8.2.0",
    "esm": "^3.2.25",
    "ethereum-waffle": "^3.0.2",
    "ethers": "^5.0.8",
    "solidity-coverage": "^0.7.10",
    "ts-node": "^8.10.2",
    "typechain": "^2.0.0",
    "typescript": "^3.9.7",
    "web3": "^1.2.0"
  },
  "devDependencies": {
    "@nomiclabs/buidler": "^1.4.8"
  }
}
